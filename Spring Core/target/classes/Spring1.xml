<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.springframework.org/schema/beans"
    xmlns:mvc="http://www.springframework.org/schema/mvc"
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-3.0.xsd" >
 
       
   <bean id="cus" class="springCoreQ1.Customer">  
	<property name="customerId" value="1"></property>
	<property name="customerName" value="Mahek Pandya"></property>
	<property name="customerContact" value="7875975997"></property>
	<property name="customerAddress" value="Nagpur"></property>
   </bean>
   <bean id="add" class="springCoreQ1.Address">
	<property name="street" value="Surendranagar"></property>
	<property name="city" value="Nagpur"></property>
	<property name="state" value="Maharashtra"></property>
	<property name="zip" value="440015"></property>
	<property name="country" value="India"></property> 
   </bean>
</beans>